<div class="created-survey-container" id="<%= @survey.id %>" >
  <ul>
    <% @questions.each do |question| %>
    <li class><%= question.prompt %></li>
      <% question.choices.each do |choice| %>
    <ul>
      <li> <%= choice.selection %>: <%= @survey.user_responses.where(choice_id: choice.id).length
%></li>
    </ul>
      <% end %>
    <% end %>
  </ul>
</div>


